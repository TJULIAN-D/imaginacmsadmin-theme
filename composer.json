{
  "name": "imagina/adminlte-theme",
  "type": "asgard-theme",
  "description": "An AsgardCMS administration theme",
  "keywords": [
    "asgard-cms"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "info@asgardcms.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0"
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan stylist:publish",
      "rm -rf ./public/iadmin",
      "cp -Rf ./Themes/Adminlte/iadmin ./public/"
    ],
    "post-update-cmd": [
      "php artisan stylist:publish",
      "rm -rf ./public/iadmin",
      "cp -Rf ./Themes/Adminlte/iadmin ./public/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "dev.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
